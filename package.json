{
  "name": "voting",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.3.4",
    "env-cmd": "^10.1.0",
    "exifr": "^7.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-charts": "^4.0.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start-dev-voting": "env-cmd -e development-voting react-scripts start",
    "build-dev-voting": "env-cmd -e development-voting react-scripts build",
    "start-staging-voting": "env-cmd -e staging-voting react-scripts start",
    "build-staging-voting": "set \"GENERATE_SOURCEMAP=false\" && env-cmd -e staging-voting react-scripts build",
    "start-production-voting": "env-cmd -e staging-voting react-scripts start",
    "build-production-voting": "set \"GENERATE_SOURCEMAP=false\" && env-cmd -e staging-voting react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
